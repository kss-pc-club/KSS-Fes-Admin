rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // match /{document=**} {
    //   allow read, write: if false;
    // }
    match /monitor/{info} {
    	allow read: if true;
      allow write: if false;
    }
    match /class_info/{classId}{
      allow read: if true;
      allow write: if false;
    }
    match /payment_log/{log}{
    	allow read, write: if false;
    }
  }
}